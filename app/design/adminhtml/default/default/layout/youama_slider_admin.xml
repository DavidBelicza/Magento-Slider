<layout version="0.1.0">
    <default>
        <reference name="head">
            <!-- delete the following one line if you have integrated jquery on admin area by another module -->
            <action method="addJs"><script>youama/jquery-1-7-1.js</script></action>
            
            <action method="addItem"><type>skin_css</type><name>youama/colorpicker/css/colorpicker.css</name></action>
            <action method="addItem"><type>skin_css</type><name>youama/colorpicker/css/layout.css</name></action>
            
            <action method="addItem"><type>skin_js</type><name>youama/colorpicker/js/colorpicker.js</name></action>
            <action method="addItem"><type>skin_js</type><name>youama/colorpicker/js/eye.js</name></action>
            <action method="addItem"><type>skin_js</type><name>youama/colorpicker/js/utils.js</name></action>
            <action method="addItem"><type>skin_js</type><name>youama/colorpicker/js/layout.js</name></action>
            
            <action method="addItem"><type>skin_js</type><name>youama/colorpicker/start.js</name></action>
        </reference>
    </default>
</layout>